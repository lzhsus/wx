/* 扫码 */
.scanCode{
  width: 100%;
  font-size: 24rpx;
  color: red;
}
/* 全局背景 */
.content{
  background-color: #fafafa;
}
/**index.wxss**/
.header-top{
  padding:20rpx 20rpx 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.address::after{
  display: block;
  clear: both;
  content: ''
}
.address-index{
  width: 50rpx;
  height: 50rpx;
  float: left;
}
.address-txt{
  font-size: 32rpx;
  line-height: 50rpx;
  height: 50rpx;
  float: left;
  display: block;
  width: 270rpx;
  /* background-color: red; */
  white-space:nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.search{
  position: relative;
}
.search-img{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 10rpx;
  width: 40rpx;
  height: 40rpx;
}
.search-input{
  padding-left: 60rpx;
  width: 260rpx;
  border: 1px solid #ccc;
  border-radius: 30rpx;
  font-size: 24rpx;
}
.evm{
  width: 62rpx;
  height: 62rpx;
}
.evm>image{
  width: 100%;
  height:100%;
}
swiper{
  height: 280rpx;
}
.swiper{
  margin-top: 20rpx;
}
.slide-image{
  border-radius: 60rpx;
  margin-left: 2%;
  width: 96%;
  height: 96%;
  box-sizing: border-box;
  box-shadow: -3rpx 5rpx 5rpx rgba(0, 0, 0, 0.2)
}
.classify-Item{
  height: 140rpx;
  /* margin-top: 10rpx; */
  width: 200%;
  text-align: center;
  position: relative;
}
.classify-Item>view{
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  position: absolute;
  top: 0;
  left: 0;
}
.more-border{
  border-radius: 20rpx;
   height:140rpx;
   width: 140rpx;
}
.more-item{
  width:100%;
  height: 100%;
  border-radius: 20rpx;
 
  padding: 4rpx;
  box-shadow: 2px 2px 2px rgba(231, 231, 231, 0.1),-2px -2px 2px rgba(230, 228, 228, 0.1)
}
.more-tit{
  width: 100%;
  text-align: center;
  display: block;
  font-size: 24rpx;
  color: rgb(100, 99, 99);
  border-radius: 10rpx;
}
.yd{
  margin-top: 10rpx;
  width: 100%;
  height: 20rpx;
  position: relative;
}
.yd>view{
  position: absolute;
  top: 50%;
  left: 50%;
  width: 14rpx;
  height: 14rpx;
  border-radius: 50%;
  background-color: #ccc;
}
.yd>view:nth-of-type(1){
  transform: translateX(-20px)
}
.yd>view.active{
  background-color: #000;
}
/* .scroll-view-item_H{
  width: 100rpx;
  background-color: red;
  height: 200rpx;
  margin-left: 20rpx;
  float: left;
} */
.ad{
  width: 100%;
  height: 208rpx;
  padding: 20rpx 20rpx 20rpx;
  border-bottom: 1px solid #ccc;
  box-sizing: border-box;
   
}
.ad image{
  height:100%;
  width: 100%;
  border-radius: 40rpx;
   box-shadow: -3rpx 4rpx 4rpx rgba(0, 0, 0, 0.2)
}
.tweet{
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding: 20rpx 40rpx;
}
.tweetimg{
  width: 100rpx;
  height: 80rpx;
}
.tweetimg image{
  width: 100%;
  height:100%;
}
.tweettit{
  margin-left: 20rpx;
  margin-right: 20rpx;
  width: 100%;
  line-height: 30rpx;
}
.tweettit text:nth-of-type(1){
  color: rgb(54, 53, 53);
  font-size: 24rpx;
}
.tweettit text:nth-of-type(2){
  color: rgb(121, 118, 118);
  font-size: 24rpx;
}
.model-title{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx;
  background-color: #b3dbf2;
}
.model-title .line{
  width: 10rpx;
  height: 40rpx;
  background-color: red;
  float: left;
  margin-right: 30rpx;
}
.model-title text{
  display: block;
  width: 100%;
  text-align: left;
  font-size: 37rpx;
  line-height: 100%;
}
.model-title .time{
  text-align: center;
  width: 500rpx;
  border-radius: 30rpx;
  height: 50rpx;
  background-color: #eccb83;
  color: #fff;
  letter-spacing: 8rpx;
}
.prompt{
  margin-top: 10rpx;
}
.promptGoods{
  border-top: 1px solid #ccc;
  display: flex;
  justify-content: space-between;
  align-items: stretch;
  padding: 20rpx 30rpx;
}
.good-img{
  padding: 10rpx;
  width: 180rpx;
  height: 140rpx;
  border: 1px solid #999;
  margin-right: 20rpx;
}
.good-img image{
  width: 100%;
  height: 100%;
}
.good-content{
  width: 100%;
  text-align: left;
  position: relative;
}
.good-tit{
  font-size: 28rpx;
  color: rgb(68, 65, 65)
}
.good-marry{
  position: absolute;
  bottom: 0;
  width: 100%;
}
.good-marry-new{
  color: rgb(255, 72, 0);
  font-size: 44rpx;
  margin-right: 10rpx;
}
.good-marry-old{
  color: #ccc;
  font-size: 30rpx;
  text-decoration: line-through;
}
.promptAdd{
  font-size: 24rpx;
  font-weight: bold;
  color: #fff;
  display: block;
  float: right;
  margin-right: 20rpx;
  margin-top: 22rpx;
  width: 60rpx;
  text-align: center;
  border-radius: 20rpx;
  background-color: rgb(255, 72, 0);
}
.promptMore{
  font-size: 32rpx;
  width: 100%;
  text-align:center;
  color: #999;
  padding: 20rpx;
}
/* 优鲜惠 */
.shopping_H{
  overflow: hidden;
}
.shopping{
    height: 370rpx;
  /* margin-top: 10rpx; */
  width: 300%;
  text-align: center;
  position: relative;
  box-sizing: border-box;
}
.filtrates{
  /* padding-top: 20rpx;
  padding-bottom: 10rpx; */
  padding: 10rpx 20rpx;
  border-radius: 20rpx;
  width: 188rpx;
  margin: 10rpx 10rpx;
  float: left;
  overflow: hidden;
  background-color: #fff;
  box-shadow: -1rpx 1rpx 1rpx rgba(130, 130, 130, 0.1),1rpx -1rpx 1rpx rgba(130, 130, 130, 0.1)
}
.filtrates-img{
  box-sizing: border-box;
  padding: 20rpx;
  width:100%;
  height: 180rpx;
  border: 1px solid rgb(233, 230, 230);
}
.filtrates-img image{
  width: 100%;
  height: 100%;
}
.filtrates-content{
  width: 100%;
  text-align: left;
  position: relative;

}
.filtrates-tit{
  font-size: 28rpx;
  color: rgb(117, 114, 114);
  height: 80rpx;
  margin: 10rpx 0;
  overflow: hidden;
  line-clamp: 2;
  text-overflow:ellipsis;
}
.filtrates-marry{
  margin-top: 10rpx;
  position: relative;
  bottom: 0;
  width: 100%;
  height: 60rpx;
  /* background-color: #ccc; */
  line-height: 30rpx;
}
.filtrates-marry-new{
  display: block;
  color: rgb(34, 32, 31);
  font-size: 30rpx;
}
.filtrates-marry-old{
  color: #ccc;
  font-size: 24rpx;
  text-decoration: line-through;
}
.filtratesAdd{
  position: absolute;
  right: 0;
  top: 0;
  padding:0 6rpx;
  font-size: 24rpx;
  color: #fff;
  border-radius: 10rpx;
  background-color: rgb(255, 72, 0);
}
.preferentialMore{
  position: absolute;
  right:10rpx;
  top: 50%;
  transform: translateY(-50%);
  font-size: 32rpx;
  color: #999;
  writing-mode: tb-rl;
  letter-spacing: 8rpx;
  /* writing-mode：tb-rl */
}
/* model-content  更多的模块 */
.model-content{
  overflow: hidden;
}
.model-content>view{
  width: 334rpx;
  margin:10rpx 20rpx;
  height: 180rpx;
  /* background-color: #000; */
  float: left;
  position: relative;
  border-radius: 30rpx;
  box-shadow: -3rpx 4rpx 4rpx rgba(0, 0, 0, 0.2),3rpx 4rpx 4rpx rgba(0, 0, 0, 0.2)
}
.model-content>view>image{
  border-radius: 30rpx;
  width: 100%;
  height: 100%;
}
.model-tit1{
  position: absolute;
  top: 30rpx;
  left: 20rpx;
  font-size: 36rpx;
  color: #fff;
  font-weight: bold;
}
.model-tit2{
  position: absolute;
  top: 74rpx;
  left: 20rpx;
  font-size: 22rpx;
  color: #fff;
  font-weight: bold;
}
/* 特殊模块 */
.model-shop-top{
  width: 100%;
  height: 200rpx;
  padding:20rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.model-shop-top>image{
  width: 100%;
  height: 100%;
  border-radius: 30rpx;
   box-shadow: -2rpx 2rpx 2rpx rgba(0, 0, 0, 0.1),2rpx -2rpx 2rpx rgba(0, 0, 0, 0.1)
}
.model-shop-img{
  border: 0;
}
.model-shopping{
  width: 100%;
}
/* 最后 */
.index-bottom{
  overflow: hidden;
  text-align: center;
  padding: 40rpx;
  font-size: 32rpx;
  color: #ccc;
}